*.pdf filter=lfs diff=lfs merge=lfs -text
*.pickle filter=lfs diff=lfs merge=lfs -text
*.html filter=lfs diff=lfs merge=lfs -text
*.jpg filter=lfs diff=lfs merge=lfs -text

docs/*.html -filter=lfs -diff=lfs -merge=lfs -text
docs/*.pdf -filter=lfs -diff=lfs -merge=lfs -text
docs/*.jpg -filter=lfs -diff=lfs -merge=lfs -text

docs/scenarios/*.html -filter=lfs -diff=lfs -merge=lfs -text
docs/scenarios/*.pdf -filter=lfs -diff=lfs -merge=lfs -text
docs/scenarios/*.jpg -filter=lfs -diff=lfs -merge=lfs -text

